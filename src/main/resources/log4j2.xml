<?xml version="1.0" encoding="utf-8" ?>
<Configuration status="off" monitorInterval="1800">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d %-5p (%F:%L) - %m%n" />
		</Console>
	</Appenders>

	<Loggers>
		<root level="debug">
			<appender-ref ref="Console" />
		</root>
	</Loggers>
</Configuration>